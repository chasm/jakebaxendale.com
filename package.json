{
  "name": "jakebaxendale.com",
  "version": "0.0.13",
  "private": true,
  "homepage": "https://jakebaxendale.com/",
  "license": "MIT",
  "author": "Charles Munat <charles.munat@gmail.com>",
  "type": "module",
  "scripts": {
    "api": "vercel dev",
    "api:dev": "vercel dev -A dev.vercel.json",
    "astro": "astro",
    "build": "astro build",
    "bump": "pnpm version patch --force",
    "bump:major": "pnpm version major --force",
    "bump:minor": "pnpm version minor --force",
    "check:exports": "pnpx find-unused-exports",
    "check:packages": "pnpx npm-check -u",
    "dep": "pnpm fmt && pnpm lint && pnpm lint:fix && pnpm check:packages && pnpm build && rm -rf .cache .history dist",
    "dep:prod": "pnpm dep:dev && vercel deploy",
    "dev": "astro dev",
    "fmt": "pnpm biome format --write ./",
    "lint": "pnpm biome check --apply-unsafe ./",
    "lint:css": "pnpm stylelint \"src/**/*.css\"",
    "lint:fix": "pnpm stylelint \"src/**/*.css\" --fix",
    "preview": "astro preview",
    "push": "git add -A && pnpm bump && git push",
    "start": "astro dev",
    "test": "pnpm test:int",
    "test:cov": "vitest run --coverage",
    "test:int": "vitest"
  },
  "devDependencies": {
    "@astrojs/check": "^0.4.1",
    "@astrojs/mdx": "^2.1.1",
    "@astrojs/sitemap": "^3.0.5",
    "@biomejs/biome": "^1.5.3",
    "@js-temporal/polyfill": "^0.4.4",
    "@sitebender/components": "^0.1.92",
    "@types/qrcode": "^1.5.5",
    "@vitest/coverage-v8": "^1.2.2",
    "astro": "^4.3.2",
    "qrcode": "^1.5.3",
    "stylelint-config-standard": "^36.0.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vitest": "^1.2.2"
  }
}
